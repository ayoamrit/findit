{
    "builds": [
      { "src": "api/*.js", "use": "@vercel/node" },
      { "src": "public/*", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/test", "dest": "api/test.js"},
      { "src": "/(admin-page\\.html|script\\.js|style\\.css|manual-urls\\.js|adminHandler\\.js)", "dest": "/public/$1" },
      { "src": "/(.*)", "dest": "public/index.html" }
    ]
}